module Styles = {
  open Css;

  let hero =
    style([
      paddingTop(rem(9.0)),
      paddingBottom(rem(9.0)),
      backgroundImage(
        url(
          "data:image/svg+xml;base64,",
        ),
      ),
    ]);
};

let component = ReasonReact.statelessComponent("Hero");

let make = children => {
  ...component,
  render: _self => <div className=Styles.hero> ...children </div>,
};